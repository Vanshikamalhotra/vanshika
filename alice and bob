#include<conio.h
#include<stdio.h>
int main()
{
  int a[3],b[3],i,alice=0,bob=0;
  FILE *fp
  fp=f reopen ("read.txt","r",stdin);
  for(i=0;i<3;i++)
  {
    scanf("%d",&a[i]);
  }
  for(i=0;i<3;i++)
  {
    scanf("%d",&b[i]);
  }
  fclose(fp);
  for(i=0;i<3;i++)
  {
    if(a[i]>b[i])
    alice++;
    else if(a[i]<b[i]
    bob++;
  }
  fp=freopen("write.txt","w",stdout);
  printf("%d%d",alice,bob);
  fclose(fp)
  return0;
  }
